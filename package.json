{
  "name": "blog-typescript",
  "version": "1.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src --max-warnings=0",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "deploy": "yarn build && next export"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*": [
      "yarn lint --fix"
    ]
  },
  "dependencies": {
    "classnames": "2.2.6",
    "date-fns": "2.16.1",
    "gray-matter": "4.0.2",
    "mongodb": "^3.6.2",
    "next": "latest",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "remark": "13.0.0",
    "remark-html": "13.0.1",
    "swr": "^0.3.6",
    "typescript": "^4.0.3"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.10",
    "@types/jest": "^25.2.2",
    "@types/mongodb": "^3.5.31",
    "@types/node": "^14.14.2",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.14.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.2",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.1.2",
    "tailwindcss": "^1.4.0"
  },
  "license": "MIT"
}
